{
    "name": "misho",
    "version": "1.0.0",
    "description": "A mutual aid platform to scale cashless options for low-income NYC residents",
    "main": "index.js",
    "scripts": {
        "start": "npm run build & tsc && nodemon server/index.ts --ignore dist/ --ignore client",
        "build": "webpack --mode=production & npm run tailwind",
        "start:dev": "npm run build-watch & npm run start-server",
        "build-watch": "webpack -w --mode=development & npm run tailwind",
        "start-server": "nodemon server/index.ts --ignore public",
        "tailwind": "tailwindcss build -i ./src/styles/globals.css -o ./public/dist/output.css",
        "test": "mocha -r ts-node/register 'tests/**/*.ts'",
        "test:dev": "DATABASE_URL=postgres://localhost/misho_test npm run test -- --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/anthonysgro/misho.git"
    },
